{
    "schemaVersion": 1,
    "id": "game-maps",
    "version": "${version}",
    "name": "Game Maps",
    "description": "A library for editing 'template' game maps.",
    "authors": [
        "Igrium", "da0nedatgotaway"
    ],
    "contact": {
        "homepage": "https://github.com/Betrayd/game-maps",
        "sources": "https://github.com/Betrayd/game-maps"
    },
    "license": "MIT",
    "icon": "assets/game-maps/icon.png",
    "environment": "*",
    "entrypoints": {
        "main": [
            "net.betrayd.gamemaps.GameMapsEntrypoint"
        ],
        "client": [
            "net.betrayd.gamemaps.GameMapsClient"
        ]
    },
    "mixins": [
        "game-maps.mixins.json",
        {
            "config": "game-maps.client.mixins.json",
            "environment": "client"
        }
    ],
    "depends": {
        "fabricloader": ">=0.15.7",
        "minecraft": "~1.20.4",
        "java": ">=17",
        "fabric-api": "*"
    },
    "custom": {
        "modmenu": {
            "badges": [
                "library"
            ]
        }
    }
}