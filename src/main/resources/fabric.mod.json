{
	"schemaVersion": 1,
	"id": "game-maps",
	"version": "${version}",
	"name": "Game Maps",
	"description": "A library for editing 'template' game maps.",
	"authors": [
		"Igrium", "da0nedatgotaway"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MIT",
	"icon": "assets/game-maps/icon.png",
	"environment": "*",
